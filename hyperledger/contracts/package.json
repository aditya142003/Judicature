{
    "name": "judicature",
    "version": "1.0.0",
    "description": "Administration of Justice",
    "main": "src/index.js",
    "scripts": {
        "lint": "eslint *.js */**.js",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "start": "fabric-chaincode-node start --peer.address localhost:7051 --chaincode-id-name mycc"
    },
    "engineStrict": true,
    "author": "adityanithariya",
    "license": "MIT",
    "dependencies": {
        "fabric-contract-api": "^2.5.4",
        "fabric-shim": "^2.5.4",
        "json-stringify-deterministic": "^1.0.12",
        "sort-keys-recursive": "^2.1.10"
    },
    "devDependencies": {
        "nyc": "^15.1.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**",
            "index.js",
            ".eslintrc.js"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
