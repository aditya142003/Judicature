{
    "name": "judicature",
    "version": "1.0.0",
    "description": "Administration of Justice",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "pretest": "npm run lint",
        "test": "nyc mocha -r ts-node/register src/**/*.spec.ts",
        "start": "fabric-chaincode-node start",
        "build": "tsc",
        "build:watch": "tsc -w",
        "prepublishOnly": "npm run build"
    },
    "engines": {
        "node": "^16.4.0"
    },
    "engineStrict": true,
    "author": "adityanithariya",
    "license": "MIT",
    "dependencies": {
        "fabric-contract-api": "2.4.0",
        "fabric-shim": "2.4.0"
    },
    "devDependencies": {
        "@types/node": "^20.8.10",
        "nyc": "^15.1.0",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "typescript": "^5.2.2"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "coverage/**",
            "dist/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
